<script>
(function() {
    'use strict';
    
    var devToolsOpened = false;
    var checkInterval = null;
    
    // æ£€æµ‹å¼€å‘è€…å·¥å…·æ˜¯å¦å¼€å¯çš„å¤šç§æ–¹æ³•
    function detectDevTools() {
        var widthThreshold = window.outerWidth - window.innerWidth > 160;
        var heightThreshold = window.outerHeight - window.innerHeight > 160;
        
        // æ–¹æ³•1: çª—å£å¤§å°å·®å¼‚æ£€æµ‹
        if (widthThreshold || heightThreshold) {
            return true;
        }
        
        // æ–¹æ³•2: è°ƒè¯•å™¨æ£€æµ‹
        var start = performance.now();
        debugger;
        var end = performance.now();
        if (end - start > 100) {
            return true;
        }
        
        // æ–¹æ³•3: æ§åˆ¶å°æ£€æµ‹
        var element = new Image();
        Object.defineProperty(element, 'id', {
            get: function() {
                return true;
            }
        });
        console.log(element);
        
        // æ–¹æ³•4: æ€§èƒ½ç›‘æµ‹
        var perfData = window.performance.memory;
        if (perfData && perfData.usedJSHeapSize > 100000000) { // 100MB
            return true;
        }
        
        return false;
    }
    
    // å…³é—­å¼€å‘è€…å·¥å…·çš„æ–¹æ³•
    function closeDevTools() {
        try {
            // æ–¹æ³•1: è§¦å‘çª—å£è°ƒæ•´ï¼ˆå¯èƒ½å…³é—­å¼€å‘è€…å·¥å…·ï¼‰
            window.resizeTo(window.screen.availWidth, window.screen.availHeight);
            
            // æ–¹æ³•2: å°è¯• blur å’Œ focus
            window.blur();
            window.focus();
            
            // æ–¹æ³•3: æ‰“å¼€æ–°çª—å£å¹¶å…³é—­å½“å‰ï¼ˆæ¿€è¿›æ–¹æ³•ï¼‰
            // var newWindow = window.open(window.location.href, '_self');
            // if (newWindow) {
            //     window.close();
            // }
            
            // æ–¹æ³•4: é‡è½½é¡µé¢
            // window.location.reload();
            
        } catch(e) {
            // é™é»˜å¤„ç†é”™è¯¯
        }
    }
    
    // å¼ºåˆ¶å…³é—­å¼€å‘è€…å·¥å…·
    function forceCloseDevTools() {
        if (detectDevTools()) {
            devToolsOpened = true;
            console.log('å¼€å‘è€…å·¥å…·å·²æ£€æµ‹åˆ°ï¼Œæ­£åœ¨å°è¯•å…³é—­...');
            closeDevTools();
            
            // å¦‚æœæ£€æµ‹åˆ°å¤šæ¬¡ï¼Œé‡‡å–æ›´æ¿€è¿›çš„æªæ–½
            setTimeout(function() {
                if (detectDevTools()) {
                    console.log('å¼€å‘è€…å·¥å…·ä»ç„¶å¼€å¯ï¼Œå°è¯•é‡è½½é¡µé¢...');
                    window.location.reload();
                }
            }, 1000);
        }
    }
    
    // æŒ‰é”®é˜»æ­¢
    function blockShortcuts(e) {
        var blockedKeys = [
            {key: 'F12', ctrl: false, shift: false},
            {key: 'I', ctrl: true, shift: true},
            {key: 'J', ctrl: true, shift: true}, 
            {key: 'C', ctrl: true, shift: true},
            {key: 'U', ctrl: true, shift: false},
            {key: 'S', ctrl: true, shift: true} // Ctrl+Shift+S
        ];
        
        for (var i = 0; i < blockedKeys.length; i++) {
            var shortcut = blockedKeys[i];
            if (e.key === shortcut.key && 
                e.ctrlKey === shortcut.ctrl && 
                e.shiftKey === shortcut.shift) {
                e.preventDefault();
                e.stopPropagation();
                e.stopImmediatePropagation();
                
                // ç«‹å³æ£€æµ‹å¹¶å°è¯•å…³é—­å¼€å‘è€…å·¥å…·
                setTimeout(forceCloseDevTools, 100);
                return false;
            }
        }
        
        // å•ç‹¬æ£€æµ‹ F12
        if (e.key === 'F12') {
            e.preventDefault();
            e.stopPropagation();
            e.stopImmediatePropagation();
            setTimeout(forceCloseDevTools, 100);
            return false;
        }
        
        return true;
    }
    
    // å³é”®é˜»æ­¢
    function blockContextMenu(e) {
        e.preventDefault();
        e.stopPropagation();
        return false;
    }
    
    // å®šæœŸæ£€æµ‹å‡½æ•°
    function periodicCheck() {
        if (detectDevTools()) {
            console.log('å®šæœŸæ£€æµ‹ï¼šå‘ç°å¼€å‘è€…å·¥å…·å·²å¼€å¯');
            forceCloseDevTools();
        }
    }
    
    // åˆå§‹åŒ–ä¿æŠ¤
    function initProtection() {
        // ç«‹å³æ£€æµ‹ä¸€æ¬¡
        setTimeout(function() {
            forceCloseDevTools();
        }, 500);
        
        // æ¯5ç§’å®šæœŸæ£€æµ‹
        checkInterval = setInterval(periodicCheck, 5000);
        
        // æ·»åŠ äº‹ä»¶ç›‘å¬
        document.addEventListener('keydown', blockShortcuts, true);
        document.addEventListener('contextmenu', blockContextMenu, true);
        
        // ç›‘å¬çª—å£å¤§å°å˜åŒ–ï¼ˆå¼€å‘è€…å·¥å…·å¯èƒ½æ”¹å˜çª—å£ï¼‰
        var lastWidth = window.innerWidth;
        var lastHeight = window.innerHeight;
        
        window.addEventListener('resize', function() {
            var widthDiff = Math.abs(window.innerWidth - lastWidth);
            var heightDiff = Math.abs(window.innerHeight - lastHeight);
            
            if (widthDiff > 100 || heightDiff > 100) {
                setTimeout(forceCloseDevTools, 300);
            }
            
            lastWidth = window.innerWidth;
            lastHeight = window.innerHeight;
        });
        
        // ç›‘å¬é¡µé¢å¯è§æ€§å˜åŒ–
        document.addEventListener('visibilitychange', function() {
            if (!document.hidden) {
                setTimeout(forceCloseDevTools, 1000);
            }
        });
        
        // æ§åˆ¶å°å¹²æ‰°
        if (window.console) {
            var methods = ['log', 'warn', 'error', 'info', 'debug', 'clear'];
            methods.forEach(function(method) {
                if (console[method]) {
                    var original = console[method];
                    console[method] = function() {
                        // è®°å½•åˆ°åå°ï¼ˆå¯é€‰ï¼‰
                        // logToServer('Console used: ' + method, arguments);
                        
                        try {
                            original.apply(console, arguments);
                        } catch(e) {
                            // é™é»˜å¤±è´¥
                        }
                    };
                }
            });
            
            // é‡å†™ console å¯¹è±¡æœ¬èº«
            Object.defineProperty(window, 'console', {
                value: console,
                writable: false,
                configurable: false
            });
        }
        
        // é˜²æ­¢åœ¨æ–°çª—å£æ‰“å¼€å¼€å‘è€…å·¥å…·
        window.open = function() {
            return null;
        };
    }
    
    // é¡µé¢åŠ è½½å®Œæˆååˆå§‹åŒ–
    if (document.readyState === 'loading') {
        document.addEventListener('DOMContentLoaded', initProtection);
    } else {
        initProtection();
    }
    
    // æä¾›æ¸…ç†å‡½æ•°ï¼ˆå¯é€‰ï¼‰
    window.disableAntiDevTools = function() {
        if (checkInterval) {
            clearInterval(checkInterval);
        }
        document.removeEventListener('keydown', blockShortcuts, true);
        document.removeEventListener('contextmenu', blockContextMenu, true);
    };
    
})();
</script>
</head>
